    id int(11) primary key auto_increment,	
	userid varchar(256),
	primary_fingerprint varchar(256),
	user_browser varchar(256),
	user_browser_version varchar(256),
	user_os varchar(256), 
	user_os_version varchar(256), 
	user_hostaddress varchar(256),
	sessionid varchar(256),
	video_category_title varchar(256), 
	video_type varchar(256), 
	video_duration float,
	session_startdt DATETIME NOT NULL,
	session_enddt DATETIME NOT NULL,
	click_count int(11),
	adplay_count int(11),
	play_count int(11),
	pause_count int(11),
	resume_count int(11),
	ab_flag tinyint(1),
	seek_count int(11),
	buffer_count int(11),
	total_buffer_time float,
	max_buffer_time float,
	usr_tot_watching_dur int(11),
	usr_act_tot_watching_dur int(11),
	ismobile tinyint(1),
	mobile_device varchar(256),
	isp_tag varchar(256),
	app_tag tinyint(1),
	three_g_tag varchar(256),
	content_type varchar(256),
	ipcountry varchar(256),
	subdivision_1 varchar(256),
	subdivision_2 varchar(256),
	ipcity varchar(256));


