sudo mysql -u root iwant_db -e "LOAD DATA INFILE '$filename' \
	INTO TABLE customers_december \
	FIELDS TERMINATED BY ',' \
	LINES TERMINATED BY '\n' \
	IGNORE 1 ROWS"



CREATE TABLE customers_december (
	id int(11) primary key auto_increment,
	calid varchar(256),
	propertyid varchar(256),
	src varchar(256),
	activity varchar(256),
	userid varchar(256),
	primary_fingerprint varchar(256),
	user_browser varchar(256),
	user_browser_version varchar(256),
	user_os varchar(256),
	user_os_version varchar(256),
	user_hostaddress varchar(256),
	sessionid varchar(256),
	original_video_content_id varchar(256),
	video_content_id varchar(256),
	video_content_title varchar(256),
	original_video_category_id varchar(256),
	video_category_id varchar(256),
	video_category_title varchar(256),
	video_type varchar(256),
	video_duration float,
	session_startdt DATETIME NOT NULL,
	session_enddt DATETIME NOT NULL,
	click_count int(11),
	adplay_count int(11), 
	play_count int(11), 
	pause_count int(11),
	resume_count int(11),
	ab_flag tinyint(1),
	seek_count int(11),
	buffer_count int(11),
	total_buffer_time float,
	max_buffer_time float,
	usr_tot_watching_dur int(11),
	usr_act_tot_watching_dur int(11),
	isp_provider varchar(256),
	ismobile tinyint(11),
	mobile_device varchar(256),
	isp_tag varchar(256),
	number_used varchar(256),
	currency varchar(256),
	amount float,
	free_access_tag varchar(256),
	producttype varchar(256),
	productname varchar(256),
	partner varchar(256),
	mobiletype varchar(256), 
	session_startdt_year varchar(256),
	session_startdt_month varchar(256),
	session_startdt_day varchar(256),
	app_tag varchar(256),
	three_g_tag varchar(256),
	content_type varchar(256),
	verified varchar(256),
	ipcountry varchar(256),
	subdivision_1 varchar(256),
	subdivision_2 varchar(256),
	ipcity varchar(256),
	sourcescript varchar(256), 
	is_known varchar(256),
	app_version varchar(256),
	modifieddate DATETIME NOT NULL);
